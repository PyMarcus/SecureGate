{
  "env": {
      "browser": true,
      "es2020": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react-hooks/recommended",
      "@rocketseat/eslint-config/react"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react-refresh"
  ],
  "globals": {
      "JSX": true
  },
  "rules": {
      "react-refresh/only-export-components": "off",
      "camelcase": "off"
  }
}
